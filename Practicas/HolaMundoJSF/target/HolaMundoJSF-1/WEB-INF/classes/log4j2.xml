<?xml version="1.0" encoding="UTF-8"?>
<configuration status="INFO">
    <Appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout patter="%d{HH:mm:ss}[%t] %-5level %logger{36} -%msg%n"></PatternLayout>           
        </Console>
    </Appenders>
    <loggers>
        <root level="info">
            <AppenderRef ref="console"/>
        </root>
    </loggers>
</configuration>
